// ==================== Phase 2: E-commerce Models ====================

// Promotion (促销表)
model Promotion {
  promotionId        Int      @id @default(autoincrement()) @map("promotion_id")
  promotionName      String   @map("promotion_name") @default("")
  promotionType      Int      @map("promotion_type") @default(0)
  promotionValue     Decimal  @map("promotion_value") @default(0.00) @db.Decimal(10, 2)
  promotionCondition Decimal  @map("promotion_condition") @default(0.00) @db.Decimal(10, 2)
  startTime          DateTime @map("start_time")
  endTime            DateTime @map("end_time")
  status             Int      @map("status") @default(1)
  sort               Int      @map("sort") @default(100)
  shopId             Int?     @map("shop_id")
  productId          Int?     @map("product_id")
  description        String   @map("description") @default("")
  createdAt          DateTime @default(now()) @map("created_at")
  updatedAt          DateTime @updatedAt @map("updated_at")

  @@map("promotions")
}

// TimeDiscount (限时折扣表)
