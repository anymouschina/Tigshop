  discountValue      Decimal  @map("discount_value") @default(0.00) @db.Decimal(10, 2)
  startTime          DateTime @map("start_time")
  endTime            DateTime @map("end_time")
  stock              Int      @map("stock") @default(0)
  buyLimit           Int      @map("buy_limit") @default(0)
  buyCount           Int      @map("buy_count") @default(0)
  status             Int      @map("status") @default(1)
  sort               Int      @map("sort") @default(100)
  description        String   @map("description") @default("")
  createdAt          DateTime @default(now()) @map("created_at")
  updatedAt          DateTime @updatedAt @map("updated_at")

  @@map("time_discounts")
}

