generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// Token tables to support auth flows in Nest implementation
model blacklistedToken {
  id        Int      @id @default(autoincrement()) @db.UnsignedInt
  token     String   @unique @db.VarChar(512)
  userId    Int      @db.UnsignedInt
  expiresAt DateTime

  @@index([userId], map: "idx_blacklisted_token_user_id")
}

model passwordResetToken {
  id        Int      @id @default(autoincrement()) @db.UnsignedInt
  token     String   @unique @db.VarChar(512)
  email     String   @db.VarChar(255)
  userId    Int      @db.UnsignedInt
  expiresAt DateTime

  @@index([userId], map: "idx_password_reset_user_id")
  @@index([email], map: "idx_password_reset_email")
}

model emailVerificationToken {
  id        Int      @id @default(autoincrement()) @db.UnsignedInt
  token     String   @unique @db.VarChar(512)
  email     String   @db.VarChar(255)
  userId    Int      @db.UnsignedInt
  expiresAt DateTime

  @@index([userId], map: "idx_email_verification_user_id")
  @@index([email], map: "idx_email_verification_email")
}

model access_log {
  id            Int     @id @default(autoincrement()) @db.UnsignedInt
  access_time   Int     @default(0) @db.UnsignedInt
  ip_address    String  @default("") @db.VarChar(15)
  product_id    Int     @default(0) @db.UnsignedMediumInt
  shop_id       Int     @default(0) @db.UnsignedMediumInt
  user_id       Int     @default(0) @db.UnsignedMediumInt
  params_id     Int     @default(0) @db.UnsignedMediumInt
  controller    String? @default("") @db.VarChar(20)
  access_domain String? @default("") @db.VarChar(100)
  access_path   String? @default("") @db.VarChar(200)

  @@index([access_time], map: "access_time")
  @@index([product_id], map: "goods_id")
  @@index([ip_address], map: "ip_address")
  @@index([shop_id], map: "shop_id")
  @@index([user_id], map: "user_id")
}

model admin_log {
  log_id     Int    @id @default(autoincrement()) @db.UnsignedInt
  log_time   Int    @default(0) @db.UnsignedInt
  user_id    Int    @default(0) @db.UnsignedTinyInt
  log_info   String @default("") @db.VarChar(255)
  ip_address String @default("") @db.VarChar(255)

  @@index([log_time], map: "log_time")
  @@index([user_id], map: "user_id")
}

model admin_msg {
  msg_id       Int     @id @default(autoincrement()) @db.UnsignedSmallInt
  msg_type     Int     @default(0) @db.UnsignedTinyInt
  send_time    Int     @default(0) @db.UnsignedInt
  is_readed    Int     @default(0) @db.UnsignedTinyInt
  shop_id      Int     @default(0) @db.UnsignedInt
  admin_id     Int     @default(0) @db.UnsignedInt
  order_id     Int     @default(0) @db.UnsignedInt
  product_id   Int     @default(0) @db.UnsignedInt
  title        String  @default("") @db.VarChar(255)
  content      String  @db.Text
  msg_link     String? @default("") @db.VarChar(255)
  related_data String? @db.Text
  vendor_id    Int?    @default(0)

  @@index([admin_id], map: "admin_id")
  @@index([product_id], map: "goods_id")
  @@index([vendor_id], map: "idx_vendor_id")
  @@index([is_readed], map: "is_readed")
  @@index([msg_type], map: "msg_type")
  @@index([order_id], map: "order_id")
  @@index([shop_id], map: "shop_id")
}

model admin_role {
  role_id        Int     @id @default(autoincrement()) @db.UnsignedSmallInt
  role_name      String  @default("") @db.VarChar(60)
